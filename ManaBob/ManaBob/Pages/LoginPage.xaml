<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ManaBob.Pages.LoginPage">
  <ContentPage.Content>
    <StackLayout Orientation="Vertical">      
      <Entry x:Name="EntryID"  Placeholder="ID" />
      <Entry x:Name="EntryPWD" Placeholder="Password" />
      
      <Button Text="WebPage" 
              Clicked="GoToWebPage"></Button>
      <Button Text="RoomListPage" 
              Clicked="GoToListPage"></Button>
      <Button Text="Back" 
              Clicked="GoToBack"></Button>
    </StackLayout>
  </ContentPage.Content>
</ContentPage>